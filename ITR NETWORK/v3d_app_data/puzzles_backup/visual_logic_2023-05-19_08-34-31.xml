            <shadow type="textMultiline" id=".YaI7+r]8^S_Dqu^XC|$">
              <field name="HIGHLIGHT_MODE">javascript</field>
              <field name="TEXT" editor-height="10" editor-width="520">// Built-in variables: app, v3d, puzzles, VARS, PROC&amp;#10;let jsonDataElement = `&lt;tr&gt;`;&amp;#10;fetch("http://localhost:90/api/inventario/ubicacion/u3")&amp;#10;    .then((res) =&gt; res.json())&amp;#10;    .then((data) =&gt; {&amp;#10;       // Initialize with the opening &lt;tr&gt; tag&amp;#10;      for (let i = 0; i &lt; data.data.length; i++) {&amp;#10;        Object.values(data.data[i]).forEach((element) =&gt; {&amp;#10;          jsonDataElement += `&lt;td&gt;${element}&lt;/td&gt;`;&amp;#10;        });&amp;#10;        jsonDataElement += `&lt;/tr&gt;&lt;tr&gt;`; // Add closing &lt;/tr&gt; tag and opening &lt;tr&gt; tag for the next row&amp;#10;      }&amp;#10;      jsonDataElement += `&lt;/tr&gt;`; // Add closing &lt;/tr&gt; tag for the last row&amp;#10;       // Resolve the Promise with the complete HTML structure&amp;#10;    })&amp;#10;    .catch((error) =&gt; {&amp;#10;      console.log("ERROR:", error);&amp;#10;      throw error; // Propagate the error by throwing it&amp;#10;    });&amp;#10;    return jsonDataElement;</field>
            </shadow>
